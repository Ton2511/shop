    max-width: 100%;
    max-height: 180px;
    object-fit: contain;
  }
  
  .a {
    text-decoration: none;
    color: inherit;
  }
  
  .category-thumbnail {
    width: 120px;
    height: 120px;
    object-fit: cover;
  }
  
  .category-title {
    margin-top: 0.5rem;
    font-size: 1rem;
    color: #333;
  }
  
  /* Swiper Customization */
  .swiper-wrapper {
    padding: 10px 0;
  }
  
  .swiper-slide {
    display: flex;
    flex-direction: column;
    align-items: center;
  }
</style>
    
    
