            <div class="section-header d-flex flex-wrap justify-content-between mb-5">
              <h2 class="section-title text-dark">Category</h2>
  
              <div class="d-flex align-items-center">
                <a href="/shop" class="btn bg-teal me-2">View All</a>
                <div class="swiper-buttons">
                  <button class="swiper-prev category-carousel-prev btn bg-teal">❮</button>
                  <button class="swiper-next category-carousel-next btn bg-teal">❯</button>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <div class="category-carousel swiper">
              <div class="swiper-wrapper">
                <% if (categories && categories.length > 0) { %>
                  <% categories.forEach(category => { %>
                    <a href="/shop/category/<%= category._id %>" class="nav-link swiper-slide text-center">
                      <% if (category.image) { %>
                        <img src="<%= category.image %>" class="rounded-circle category-thumbnail" alt="<%= category.name %>">
                      <% } else { %>
                        <div class="rounded-circle bg-light d-flex justify-content-center align-items-center category-thumbnail">
                          <i class="bi bi-collection fs-1 text-muted"></i>
                        </div>
                      <% } %>
                      <h4 class="fs-6 mt-3 fw-normal category-title"><%= category.name %></h4>
                    </a>
                  <% }) %>
                <% } %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <section class="pb-2">
      <div class="container-lg">
